{
    "name": "Resume Centrakuzer Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "./popup/downloadPop.html",
      "default_icon": "./icons/programmer.png"
    },
    "content_scripts": [
        {
          "js": ["./scripts/linkedinContent.js"],
          "matches": [
            "https://www.linkedin.com/jobs/search/*"
          ]
        }
      ],
    "permissions": [
      "activeTab",
      "storage"
    ]
}
